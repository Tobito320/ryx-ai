{
  "ollama_base_url": "http://localhost:11434",
  "tiers": {
    "fast": {
      "model": "mistral:7b",
      "fallbacks": ["llama2-uncensored:7b", "qwen2.5:3b"],
      "description": "Fast responses for simple queries",
      "max_tokens": 1024,
      "timeout_seconds": 20,
      "use_cases": ["simple_chat", "quick_answers", "file_operations"]
    },
    "balanced": {
      "model": "qwen2.5-coder:14b",
      "fallbacks": ["deepseek-coder-v2:16b", "mistral:7b"],
      "description": "Default coding model for most tasks",
      "max_tokens": 4096,
      "timeout_seconds": 60,
      "use_cases": ["code_edit", "config_edit", "system_task", "general_coding"]
    },
    "powerful": {
      "model": "deepseek-coder-v2:16b",
      "fallbacks": ["qwen2.5-coder:14b"],
      "description": "Strong coder for complex tasks",
      "max_tokens": 8192,
      "timeout_seconds": 90,
      "use_cases": ["complex_refactor", "architecture", "multi_file_changes"]
    },
    "ultra": {
      "model": "SimonPu/Qwen3-Coder:30B-Instruct_Q4_K_XL",
      "fallbacks": ["deepseek-coder-v2:16b", "qwen2.5-coder:14b"],
      "description": "Heavy reasoning and architecture",
      "max_tokens": 16384,
      "timeout_seconds": 180,
      "use_cases": ["deep_analysis", "system_design", "major_refactoring"]
    },
    "uncensored": {
      "model": "huihui_ai/gpt-oss-abliterated:20b",
      "fallbacks": ["gpt-oss:20b", "llama2-uncensored:7b"],
      "description": "Uncensored personal reflection",
      "max_tokens": 8192,
      "timeout_seconds": 120,
      "use_cases": ["personal_chat", "uncensored_conversation"]
    }
  },
  "default_tier": "balanced",
  "intent_to_tier_mapping": {
    "CHAT": "fast",
    "CODE_EDIT": "balanced",
    "CONFIG_EDIT": "balanced",
    "FILE_OPS": "fast",
    "WEB_RESEARCH": "balanced",
    "SYSTEM_TASK": "balanced",
    "KNOWLEDGE": "fast",
    "PERSONAL_CHAT": "uncensored"
  },
  "complexity_upgrades": {
    "threshold": 0.7,
    "fast_to": "balanced",
    "balanced_to": "powerful"
  },
  "banned_primary_models": [
    "qwen2.5:3b",
    "qwen2.5:1.5b",
    "phi3:mini",
    "llama3.2:1b",
    "deepseek-coder:6.7b"
  ]
}
