  "search_commands": {
    "find_by_name": "find {directory} -name '{pattern}' 2>/dev/null",
    "find_by_name_case_insensitive": "find {directory} -iname '{pattern}' 2>/dev/null",
    "find_by_content": "grep -r '{pattern}' {directory} 2>/dev/null",
    "find_recent": "find {directory} -mtime -{days} -type f 2>/dev/null",
    "locate": "locate '{pattern}'",
    "fd_find": "fd '{pattern}' {directory}",
    "ripgrep": "rg '{pattern}' {directory}"
  },
  "common_file_patterns": {
    "great_wave": ["*great*wave*", "*greatwave*", "*wave*great*"],
    "config": ["*.conf", "*.cfg", "*.ini", "config.*", "*.config"],
    "image": ["*.png", "*.jpg", "*.jpeg", "*.gif", "*.svg", "*.webp"],
    "video": ["*.mp4", "*.mkv", "*.webm", "*.avi", "*.mov"],
    "audio": ["*.mp3", "*.wav", "*.flac", "*.ogg", "*.m4a"],
    "document": ["*.pdf", "*.doc", "*.docx", "*.odt", "*.txt"],
    "code": ["*.py", "*.js", "*.ts", "*.rs", "*.go", "*.c", "*.cpp"]
  },
  "search_locations": {
    "home": "~",
    "downloads": "~/Downloads",
    "documents": "~/Documents",
    "pictures": "~/Pictures",
    "videos": "~/Videos",
    "music": "~/Music",
    "desktop": "~/Desktop",
    "projects": "~/projects",
    "config": "~/.config",
    "system": "/"
  }
}
