{
  "system": "arch_linux",
  "description": "Arch Linux system knowledge for Ryx AI",
  "config_paths": {
    "hyprland": "~/.config/hypr/hyprland.conf",
    "hyprland_conf": "~/.config/hypr/hyprland.conf",
    "waybar": "~/.config/waybar/config",
    "waybar_style": "~/.config/waybar/style.css",
    "kitty": "~/.config/kitty/kitty.conf",
    "alacritty": "~/.config/alacritty/alacritty.yml",
    "nvim": "~/.config/nvim/init.lua",
    "neovim": "~/.config/nvim/init.lua",
    "zsh": "~/.zshrc",
    "zshrc": "~/.zshrc",
    "bash": "~/.bashrc",
    "bashrc": "~/.bashrc",
    "fish": "~/.config/fish/config.fish",
    "starship": "~/.config/starship.toml",
    "rofi": "~/.config/rofi/config.rasi",
    "wofi": "~/.config/wofi/config",
    "dunst": "~/.config/dunst/dunstrc",
    "mako": "~/.config/mako/config",
    "sway": "~/.config/sway/config",
    "i3": "~/.config/i3/config",
    "polybar": "~/.config/polybar/config.ini",
    "picom": "~/.config/picom/picom.conf",
    "gtk3": "~/.config/gtk-3.0/settings.ini",
    "gtk4": "~/.config/gtk-4.0/settings.ini",
    "fontconfig": "~/.config/fontconfig/fonts.conf",
    "mpv": "~/.config/mpv/mpv.conf",
    "ranger": "~/.config/ranger/rc.conf",
    "yazi": "~/.config/yazi/yazi.toml",
    "tmux": "~/.tmux.conf",
    "ssh": "~/.ssh/config",
    "git": "~/.gitconfig",
    "pacman": "/etc/pacman.conf",
    "grub": "/etc/default/grub",
    "fstab": "/etc/fstab",
    "hosts": "/etc/hosts"
  },
  "common_directories": {
    "home": "~",
    "config": "~/.config",
    "cache": "~/.cache",
    "local": "~/.local",
    "bin": "~/.local/bin",
    "downloads": "~/Downloads",
    "documents": "~/Documents",
    "pictures": "~/Pictures",
    "videos": "~/Videos",
    "music": "~/Music",
    "projects": "~/projects",
    "repos": "~/repos"
  },
  "package_manager": {
    "install": "sudo pacman -S",
    "update": "sudo pacman -Syu",
    "search": "pacman -Ss",
    "remove": "sudo pacman -Rns",
    "info": "pacman -Qi",
    "list_installed": "pacman -Q",
    "aur_helper": "yay"
  },
  "common_commands": {
    "update_system": "sudo pacman -Syu",
    "install_package": "sudo pacman -S {package}",
    "search_package": "pacman -Ss {query}",
    "remove_package": "sudo pacman -Rns {package}",
    "clean_cache": "sudo pacman -Sc",
    "list_orphans": "pacman -Qdt",
    "remove_orphans": "sudo pacman -Rns $(pacman -Qdtq)",
    "reload_hyprland": "hyprctl reload",
    "restart_waybar": "killall waybar && waybar &",
    "check_logs": "journalctl -xe",
    "system_info": "neofetch"
  },
  "aliases": {
    "hypr": "hyprland",
    "hyprland config": "hyprland",
    "waybar config": "waybar",
    "waybar style": "waybar_style",
    "kitty config": "kitty",
    "nvim config": "nvim",
    "neovim config": "nvim",
    "vim config": "nvim",
    "zsh config": "zsh",
    "bash config": "bash",
    "terminal config": "kitty",
    "shell config": "zsh"
  },
  "websites": {
    "youtube": "https://youtube.com",
    "yt": "https://youtube.com",
    "github": "https://github.com",
    "gh": "https://github.com",
    "reddit": "https://reddit.com",
    "google": "https://google.com",
    "duckduckgo": "https://duckduckgo.com",
    "ddg": "https://duckduckgo.com",
    "twitter": "https://twitter.com",
    "x": "https://x.com",
    "linkedin": "https://linkedin.com",
    "stackoverflow": "https://stackoverflow.com",
    "so": "https://stackoverflow.com",
    "archlinux": "https://archlinux.org",
    "archwiki": "https://wiki.archlinux.org",
    "aur": "https://aur.archlinux.org",
    "gmail": "https://mail.google.com",
    "proton": "https://mail.proton.me",
    "netflix": "https://netflix.com",
    "spotify": "https://open.spotify.com",
    "twitch": "https://twitch.tv",
    "discord": "https://discord.com/app",
    "chatgpt": "https://chat.openai.com",
    "claude": "https://claude.ai",
    "arch wiki": "https://wiki.archlinux.org"
  },
  "browsers": [
    "firefox",
    "chromium",
    "brave",
    "qutebrowser"
  ],
  "default_browser": "firefox",
  "editors": [
    "nvim",
    "vim",
    "nano",
    "code",
    "kate",
    "gedit"
  ],
  "default_editor": "nvim",
  "terminals": [
    "kitty",
    "alacritty",
    "foot",
    "wezterm"
  ],
  "default_terminal": "kitty"
}
